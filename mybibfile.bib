@article{maiko,
abstract = {A new active target named MAIKo (Mu-PIC based Active target for Inverse Kinematics∘) has been developed at Kyoto University and Research Center for Nuclear Physics (RCNP), Osaka University. MAIKo is suited for missing-mass spectroscopy of unstable nuclei at forward scattering angles in inverse kinematics. MAIKo consists of a time projection chamber (TPC), which incorporates a micro-pixel chamber ($\mu$-PIC) as the electron multiplication and collection system. In MAIKo, the medium gas also plays the role of a reaction target, thus allowing detection of low-energy recoil particles with high position resolution. The MAIKo TPC was commissioned with He(93{\%})+iso-C4H10(7{\%}) and He(93{\%})+CO2(7{\%}) mixture gasses at 430 hPa. The gas gain and the angular resolution of MAIKo were evaluated with an alpha source and a 4He beam at 56 MeV. The TPC was stably operated up to 1000-kcps beam intensity. A tracking algorithm using the Hough transform method has been developed to analyze scattering events. An angular resolution of 1.3°was achieved for scattered 4He particles.},
author = {Furuno, T. and Kawabata, T. and Ong, H. J. and Adachi, S. and Ayyad, Y. and Baba, T. and Fujikawa, Y. and Hashimoto, T. and Inaba, K. and Ishii, Y. and Kabuki, S. and Kubo, H. and Matsuda, Y. and Matsuoka, Y. and Mizumoto, T. and Morimoto, T. and Murata, M. and Sawano, T. and Suzuki, T. and Takada, A. and Tanaka, J. and Tanihata, I. and Tanimori, T. and Tran, D. T. and Tsumura, M. and Watanabe, H. D.},
doi = {10.1016/j.nima.2018.08.042},
file = {:home/takanobu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Furuno et al. - 2018 - Performance test of the MAIKo active target.pdf:pdf},
issn = {01689002},
journal = {Nuclear Instruments and Methods in Physics Research, Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
keywords = {Active target,Hough transformation,MAIKo,Missing-mass spectroscopy,Time projection chamber,$\mu$-PIC},
mendeley-groups = {NIM},
number = {May},
pages = {215--224},
publisher = {Elsevier Ltd},
title = {{Performance test of the MAIKo active target}},
url = {https://doi.org/10.1016/j.nima.2018.08.042},
volume = {908},
year = {2018}
}

