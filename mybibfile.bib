file = {:home/takanobu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Beard, Austin, Cyburt - 2017 - Enhancement of the Triple Alpha Rate in a Hot Dense Medium.pdf:pdf},
