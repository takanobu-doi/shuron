}

@article{kondoetal,
abstract = {We carried out a detailed measurement of the neutron-induced or-particle emission double-differential cross section of carbon at 14.2MeV, for which there are few measured data in spite of its importance in many applications. In our measurement, a superior S/N ratio, high angular/energy resolutions and a wide detection energy range were realized with a pencil DT neutron beam and a countertelescope system. The obtained cross section for the 12 C(n, $\alpha$ 0 ) 9 Be( ground state ) reaction agreed well with the results of previous experiments and evaluated nuclear data. The obtained angular-differential cross section of the 12 C(n, n′+3$\alpha$) reaction for $\alpha$-particles showed a strong forward-peaked distribution that suggested a significant contribution of the direct reaction process to the 3a breakup. We attempted to calculate the emitted particle spectra by a Monte Carlo method and estimate the branching ratio of the channels that contribute to the 12 C(n,n′+3$\alpha$) reaction. As a result, it was found that the 12 C(n,$\alpha$) 9 Be* channels play an important role in generating the experimental double-differential cross section both of emitted $\alpha$-particles and neutrons. The estimated ratio of the 12 C(n,$\alpha$) 9 Be* channels was approximately 40{\%}, somewhat larger than those evaluated in previous studies. {\textcopyright} Atomic Energy Society of Japan.},
author = {Kondo, Keitaro and Murata, Isao and Ochiai, Kentaro and Kubota, Naoyoshi and Miyamaru, Hiroyuki and Konno, Chikara and Nishitani, Takeo},
doi = {10.1080/18811248.2008.9711420},
file = {:home/takanobu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kondo et al. - 2008 - Measurement and analysis of neutron-induced alpha particle emission double-differential cross section of carbon at.pdf:pdf},
issn = {00223131},
journal = {Journal of Nuclear Science and Technology},
keywords = {14-MeV neutron,Alpha particle,Breakup reaction,Carbon,Double-differential cross section,Fusion reactor},
mendeley-groups = {12C(n*),12C(n*)/Triple alpha},
number = {2},
pages = {103--115},
title = {{Measurement and analysis of neutron-induced alpha particle emission double-differential cross section of carbon at 14.2 MeV}},
volume = {45},
year = {2008}
}

