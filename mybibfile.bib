@article{active-tpc,
  abstract = {Weakly bound nuclear systems can be considered to represent a good testing-ground of our understanding of non-perturbative quantum systems. Reactions leading to bound and unbound states in systems with very unbalanced neutron-to-proton ratios are used to understand the properties of these systems. Radioactive beams with energies from below the Coulomb barrier up to several hundreds MeV/nucleon are now available, and with these beams, a broad variety of studies of nuclei near the drip-line can be performed. To compensate for the low intensity of secondary beams as compared to primary beams, thick targets and high efficiency detection is necessary. In this context, a new generation of detectors was developed, called active target detectors: the detector gas is used as target, and the determination of the reaction vertex in three dimensions allows for good resolution even with thick targets. The reaction products can be measured over essentially 4$\pi$. The physics explored with these detectors together with the technology developed will be described.},
  author = {Beceiro-Novo, S. and Ahn, T. and Bazin, D. and Mittig, W.},
  doi = {10.1016/j.ppnp.2015.06.003},
  file = {:home/takanobu/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Beceiro-Novo et al. - 2015 - Active targets for the study of nuclei far from stability.pdf:pdf},
  issn = {01466410},
  journal = {Progress in Particle and Nuclear Physics},
  keywords = {Active targets,Gas detectors,Inverse kinematics},
  pages = {124--165},
  publisher = {Elsevier B.V.},
  title = {{Active targets for the study of nuclei far from stability}},
  url = {http://dx.doi.org/10.1016/j.ppnp.2015.06.003},
  volume = {84},
  year = {2015}
}
