journal = {Journal of Nuclear Science and Technology},
keywords = {14 MeV,25.4 cm diam. NE213,8.3 m TOF,Aluminum,Carbon,Comparative evaluations,Copper,Counting statistics,Cylindrical sample,Data,Double differential cross sections,ENDF/B-IV,Energy resolution,Fluorine,JENDL-3PR2,Lead,Magnesium,Measuring methods,Neutron emission,Partial differential cross sections,Silicon,Vanadium},
